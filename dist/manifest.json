{
   "background": {
      "scripts": [ "background.js" ]
   },
   "commands": {
      "copy-title-to-clipboard": {
         "description": "Copy title to clipboard",
         "suggested_key": {
            "default": "Ctrl+Shift+V",
            "mac": "Command+Shift+V"
         }
      },
      "go-to-issue": {
         "description": "Open the issue page",
         "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "Command+Shift+F"
         }
      },
      "refresh-issue-table": {
         "description": "Refresh issue table",
         "suggested_key": {
            "default": "Alt+R",
            "mac": "Alt+R"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "jquery.min.js", "content_script.js" ],
      "matches": [ "*://*/*" ]
   } ],
   "description": "This is a Jira extension that will help you a lot",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "WebcodEx",
   "options_page": "options.html",
   "permissions": [ "activeTab", "tabs", "contextMenus", "clipboardRead", "clipboardWrite", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}
